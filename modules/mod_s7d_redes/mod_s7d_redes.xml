<?xml version="1.0" encoding="utf-8"?><extension	type="module"	version="1.7"	client="site"	method="upgrade">	<name>S7D Redes</name>	<author>Carlos </author>	<creationDate>Julho 2015</creationDate>	<copyright>Copyright (C) 2015 - Site 7 Dias</copyright>	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>	<authorEmail>carlos@ibsweb.com.br</authorEmail>	<authorUrl>www.site7dias.com.br</authorUrl>	<version>1.0</version>	<description>Exibir Redes Sociais</description>	<files>		<filename module="mod_s7d_redes">mod_s7d_redes.php</filename>		<folder>assets</folder>		<folder>elements</folder>		<folder>tmpl</folder>		<filename>helper.php</filename>		<filename>index.html</filename>		<filename>mod_s7d_redes.xml</filename>	</files>		<help key="JHELP_EXTENSIONS_MODULE_MANAGER_SYNDICATION_FEEDS" />	<updateservers>       <server type="extension" priority="1" name="S7D Redes Update">http://extensions.site7dias.com.br/extensions/updates/mod_s7d_redes.xml</server>    </updateservers>	<config>		<fields name="params">					<fieldset name="basic"  addfieldpath="/modules/mod_s7d_redes/elements">						<field			name="linkFace"			type="text"			label="Facebook"			hint="Link"			default="#"			description="Link Facebook" />			<field			name="linkYou"			type="text"			label="YouTube"			hint="Link"			description="Link YouTube" />			<field			name="linkTwi"			type="text"			hint="Link"			label="Twitter"			description="Link Twitter" />			<field			name="linkGplus"			type="text"			hint="Link"			label="G+"			description="Link Google Plus" />			<field			name="linkInst"			type="text"			hint="Link"			label="Instagram"			description="Link Instagram" />			<field			name="linkLink"			type="text"			hint="Link"			label="Linkedin"			description="Link Linkedin" />			<field			name="linkWhats"			type="text"			label="Whatsapp"			hint="ex: (xx) xxxx-xxxx"			description="Telefone Whatsapp" />									</fieldset>				<fieldset				name="s7dconfig" label="Configurações">				<field 			type="sspace" 			name="myspacer" 			class="lap"			label="Configurações"			/>				<field name="sSize" type="list" default="" label="Tamanho" description="">			<option value="08">08</option>			<option value="10">10</option>			<option value="12">12</option>			<option value="14">14</option>			<option value="16">16</option>			<option value="18">18</option>			<option value="20">20</option>			<option value="22">22</option>			<option value="24">24</option>			<option value="26">26</option>			<option value="28">28</option>			<option value="30">30</option>			<option value="32">32</option>			<option value="34">34</option>			<option value="36">36</option>			<option value="38">38</option>			<option value="40">40</option>			<option value="42">42</option>			<option value="44">44</option>			</field>			<field name="sStyle" type="list" default="" label="Style" description="">			<option value="default">Default</option>			<option value="square">Square</option>			</field>			</fieldset>										<fieldset				name="advanced">				<field					name="layout"					type="modulelayout"					label="JFIELD_ALT_LAYOUT_LABEL"					description="JFIELD_ALT_MODULE_LAYOUT_DESC" />				<field					name="moduleclass_sfx"					type="text"					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />				<field					name="cache"					type="list"					default="0"					label="COM_MODULES_FIELD_CACHING_LABEL"					description="COM_MODULES_FIELD_CACHING_DESC">					<option						value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>				</field>			</fieldset>		</fields>	</config></extension>