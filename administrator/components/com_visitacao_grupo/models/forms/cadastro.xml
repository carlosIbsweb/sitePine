    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_VISITACAO_GRUPO_FORM_LBL_CADASTRO_CREATED_BY" description="COM_VISITACAO_GRUPO_FORM_DESC_CADASTRO_CREATED_BY" hidden="true" hint="COM_VISITACAO_GRUPO_FORM_LBL_CADASTRO_CREATED_BY"/>
