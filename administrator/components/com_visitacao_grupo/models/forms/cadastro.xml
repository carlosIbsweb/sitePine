    <field name="nome" filter="safehtml" type="text" readonly="true" label="COM_VISITACAO_GRUPO_FORM_LBL_CADASTRO_NOME" description="COM_VISITACAO_GRUPO_FORM_DESC_CADASTRO_NOME" hint="COM_VISITACAO_GRUPO_FORM_LBL_CADASTRO_NOME"/>
