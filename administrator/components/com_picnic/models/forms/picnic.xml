    <field name="created_by" type="createdby" default="0" label="COM_PICNIC_FORM_LBL_PICNIC_CREATED_BY" description="COM_PICNIC_FORM_DESC_PICNIC_CREATED_BY" hidden="true" hint="COM_PICNIC_FORM_LBL_PICNIC_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_PICNIC_FORM_LBL_PICNIC_MODIFIED_BY" description="COM_PICNIC_FORM_DESC_PICNIC_MODIFIED_BY" hidden="true" hint="COM_PICNIC_FORM_LBL_PICNIC_MODIFIED_BY"/>
    <field name="nome" default="255" filter="safehtml" type="text" readonly="true" label="COM_PICNIC_FORM_LBL_PICNIC_NOME" description="COM_PICNIC_FORM_DESC_PICNIC_NOME" hint="COM_PICNIC_FORM_LBL_PICNIC_NOME"/>
    <field name="telefone" default="255" filter="safehtml" type="text" readonly="true" label="COM_PICNIC_FORM_LBL_PICNIC_TELEFONE" description="COM_PICNIC_FORM_DESC_PICNIC_TELEFONE" hint="COM_PICNIC_FORM_LBL_PICNIC_TELEFONE"/>
    <field name="email" default="255" filter="safehtml" type="text" readonly="true" label="COM_PICNIC_FORM_LBL_PICNIC_EMAIL" description="COM_PICNIC_FORM_DESC_PICNIC_EMAIL" hint="COM_PICNIC_FORM_LBL_PICNIC_EMAIL"/>
    <field name="numeroadult" default="255" filter="safehtml" type="text" readonly="true" label="COM_PICNIC_FORM_LBL_PICNIC_NUMEROADULT" description="COM_PICNIC_FORM_DESC_PICNIC_NUMEROADULT" hint="COM_PICNIC_FORM_LBL_PICNIC_NUMEROADULT"/>
    <field name="numerocria" default="255" filter="safehtml" type="text" readonly="true" label="COM_PICNIC_FORM_LBL_PICNIC_NUMEROCRIA" description="COM_PICNIC_FORM_DESC_PICNIC_NUMEROCRIA" hint="COM_PICNIC_FORM_LBL_PICNIC_NUMEROCRIA"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
